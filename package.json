{
    "name": "bayswaterpc-superplate-blog",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
        "build:analyze": "ANALYZE=true npm run build",
        "apollo:download-schema": "apollo client:download-schema schema.graphql",
        "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
        "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
        "codegen:generate": "graphql-codegen",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "test": "jest -c test/jest.config.js",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --config video=false",
        "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
    },
    "dependencies": {
        "next": "10.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "next-compose-plugins": "^2.2.1",
        "@chakra-ui/react": "^1.1.4",
        "@emotion/react": "^11.1.4",
        "@emotion/styled": "^11.0.0",
        "framer-motion": "^3.2.1",
        "sass": "^1.32.5",
        "@next/bundle-analyzer": "^10.0.5",
        "@apollo/client": "^3.3.7",
        "graphql": "^15.4.0",
        "graphql-request": "^3.4.0",
        "react-query": "^3.5.16",
        "mobx": "^6.0.4",
        "mobx-react": "^7.0.5",
        "mobx-state-tree": "^5.0.1",
        "next-i18next": "^7.0.1",
        "prettier": "^2.2.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.13",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "typescript": "^4.1.3",
        "@svgr/cli": "^5.5.0",
        "apollo": "^2.32.1",
        "@graphql-codegen/cli": "^1.20.0",
        "@graphql-codegen/typescript": "^1.20.0",
        "@graphql-codegen/typescript-operations": "^1.17.13",
        "eslint": "^7.15.0",
        "@typescript-eslint/eslint-plugin": "^4.9.1",
        "@typescript-eslint/parser": "^4.9.1",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-prettier": "^3.2.0",
        "@testing-library/react": "^11.2.3",
        "@testing-library/react-hooks": "^5.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.4.4",
        "nock": "^13.0.6",
        "isomorphic-unfetch": "^3.1.0",
        "dotenv": "^8.2.0",
        "cypress": "^6.2.1",
        "start-server-and-test": "^1.11.0"
    }
}
